{
  "name": "static-rewrite",
  "type": "static",
  "description": "Static website example.",
  "headers": {
    "*": {
      "X-Foo": "1",
      "X-Baz": "2",
      "X-Type": "default"
    },
    "*.css": {
      "X-Type": "css"
    }
  },
  "redirects": {
    "/blog": {
      "location": "https://blog.apex.sh/",
      "status": 302
    },
    "/store/:brand/products/:product": {
      "location": "/shop/:brand/:product",
      "status": 302
    },
    "/*": {
      "location": "/",
      "status": 200
    }
  }
}
